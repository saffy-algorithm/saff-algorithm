def dfs(idx, cur_s):
    global ans
    if cur_s >= B:
        ans = min(ans, cur_s)
        return
    
    if idx == N:
        return
    
    if cur_s >= ans:
        return

    dfs(idx+1, cur_s+S[idx])
    dfs(idx+1, cur_s)

T = int(input())
for tc in range(1, T+1):
    N, B = map(int, input().split())
    assist = list(map(int, input().split()))
    S = sorted(assist)
    ans = sum(assist)
    dfs(0, 0)
    print(f'#{tc} {ans-B}')